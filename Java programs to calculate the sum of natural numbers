package javaapplication15;

/**
 *
 * @author Radhika
 */
public class JavaApplication15 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        int num = 600, sum = 0;
        for(int i = 1; i<=num; ++i)
        {
            sum += i;
        }
        System.out.println("sum = "+ sum);
        
        }
    }
    

